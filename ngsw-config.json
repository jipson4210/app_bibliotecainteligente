{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/**/*.module.js",
          "/**/*.module.js.map"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/assets/**",
          "/**/.*",
          "/**/*",
          "!/**/*.map"
        ],
        "versionedFiles": []
      },
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 104857600,
        "maxAge": "30d"
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "/api/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 104857600,
        "maxAge": "1h"
      }
    },
    {
      "name": "api-freshness",
      "urls": [
        "/api/users**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout": "5s",
        "maxAge": "5m"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*"
  ],
  "navigationRequestStrategy": "network-first"
}
